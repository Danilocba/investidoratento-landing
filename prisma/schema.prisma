generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id         String   @id @default(cuid())
  email      String   @unique
  confirmed  Boolean  @default(false)
  token      String   @unique
  tokenExp   DateTime @default(now())
  createdAt  DateTime @default(now())
}